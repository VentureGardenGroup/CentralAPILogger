<configuration>
    <configSections>
      <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
   </configSections>
 <nlog>
    <extensions>
      <add assembly="StackifyLib.nLog" />
    </extensions>
    <targets>
      <target name="stackify" type="StackifyTarget" />
    </targets>
    <rules>
      <logger name="*" minlevel="Info" writeTo="stackify" />
    </rules>
  </nlog>
</configuration>